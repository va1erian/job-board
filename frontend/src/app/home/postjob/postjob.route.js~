import Controller from './newarticle.controller.js';

export default function route($stateProvider) {
    'ngInject';

    $stateProvider
        .state('home.newarticle', {
            url: '/newarticle',
            templateUrl: 'app/home/newarticle/newarticle.html',
            controller: Controller,
            controllerAs: 'vm',
        });
}
